{
    "name": "Basic Chrome Extension",
    "version": "1.0",
    "description": "Displays an overlay and routes the user to a new website",
    "permissions": ["declarativeContent", "storage"],
    "background": {
        "scripts": ["index.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "index.html"
    },
    "devtools_page": "devtools.html",
    "manifest_version": 2
}